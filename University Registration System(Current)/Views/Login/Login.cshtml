
@{
    Layout = null;
}
<head>

</head>
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Login</title>
    <link rel="stylesheet" href="~/css/LoginCSS.css">
    <script src="~/Scripts/web script/loginJS.js" type="text/javascript"></script>

    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css">
</head>
<body>

    
    
    <div id="form_info">
        <h1 id="title_university">Welcome to XYZ university</h1>
        <form>

            <h1>Login</h1><br>

            <label for="email">YOUR EMAIL</label>
            <input type="email" id="email" name="email" required>

            <label for="pword">YOUR PASSWORD</label>
            <input type="password" id="pword" name="pword" required>

            <button onclick=login_button() id="btnMain">Log In</button><br>
        </form>
        <div id="register_div">
            <button onclick="window.location.href = '/register/Register';" id="btnMinor">New User? Register Now</button>
        </div>

    </div>

</body>

</html>